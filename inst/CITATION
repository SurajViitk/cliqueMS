citHeader("To cite 'cliqueMS' package in publications use:")

citEntry(entry="inproceedings",
	 author=c(
	 person("O.","Senan"), person("A.","Aguilar-Mogas"),
	 person("M.","Navarro"),
	 person("J.","Capellades"),
	 person("L", "Noon"),
	 person("D", "Burks"),
	 person("O.","Yanes"),
	 person("R.","Guimerà"), person("M.","Sales-Pardo")),
	 title="CliqueMS: a computational tool for annotating in-source metabolite ions from LC-MS untargeted metabolomics data based on a coelution similarity network",
	 journal="Bioinformatics",
	 year="2019",
	 month="March",
	 url="https://doi.org/10.1093/bioinformatics/btz207"
	 textVersion=paste("O. Senan, A. Aguilar-Mogas, M. Navarro, O. Yanes, R. Guimerà and M. Sales-Pardo",
	 "CliqueMS: a computational tool for annotating in-source metabolite ions from LC-MS untargeted metabolomics data based on a coelution similarity network",
	 "Bioinformatics, (2019), ")
	 )
	 
